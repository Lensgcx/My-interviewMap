<div class="navbar-default" role="navigation">
    <div class="sidebar-nav navbar-collapse">
        <ul class="nav side-menu-list">
            {{#each menuData}}
            {{! ============ first each loop ============ }}
                <li class="side-menu-item">
                    <!--<a href="./pages/about.html"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>-->
                    <a href="javescript:void(0)" data-toggle="tab" data-href="{{path}}">
                        <i class="fa fa-fw {{icon}}"></i>
                        {{name}}
                        <span class="fa arrow"></span>
                    </a>
                    {{#if children}}
                        <ul class="nav nav-second-level">
                            {{#each children}}
                            {{! ============ second each loop ============ }}
                                <li class="side-menu-item">
                                    <a href="javescript:void(0)" data-toggle="tab" data-href="{{path}}">
                                        <i class="fa fa-fw {{icon}}"></i>
                                        {{name}}
                                    </a>
                                </li>
                            {{/each}}
                        </ul>
                    {{/if}}
                </li>
            {{/each}}
        </ul>
    </div>
</div>
