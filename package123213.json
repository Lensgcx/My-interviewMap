{
  "name": "My-interviewMap",
  "author": "Broccoli spring( gcx ) <Lensgcx@163.com>",
  "version": "1.0.1",
  "description": "A clean Webpack 4 + Bootstrap 4 + Handlebars frontend project for My interviewMap.",
  "scripts": {
    "build2": "webpack --config build/webpack-prod.config.js --colors --optimize-minimize",
    "dev": "webpack-dev-server --watch --colors --inline --progress --config build/webpack-dev.config.js --mode development",
    "before": "webpack-dev-server --config build/webpack-dev.config.js --watch --colors -d",
    "build": "cross-env NODE_ENV=production node build/build.js"
  },
  "keywords": [
    "frontend",
    "es6",
    "javascript",
    "bootstrap",
    "handlebars",
    "webpack",
    "starter-kit"
  ],
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "handlebars": "^4.0.11",
    "handlebars-layouts": "^3.1.4",
    "handlebars-loader": "^1.7.0",
    "hbs": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ora": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-strip-inline-comments": "^0.1.5",
    "postcss-url": "^8.0.0",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.20.3",
    "svg-sprite-loader": "^3.9.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "bootstrap": "^3.3.7"
  },
  "homepage": "https://github.com/Lensgcx/My-interviewMap/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lensgcx/My-interviewMap.git"
  },
  "bugs": {
    "url": "https://github.com/Lensgcx/My-interviewMap/issues"
  }
}
